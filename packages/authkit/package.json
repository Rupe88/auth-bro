{
  "name": "auth-bro",
  "version": "1.0.0",
  "description": "Production-ready authentication library with Prisma support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "keywords": [
    "authentication",
    "auth",
    "jwt",
    "prisma",
    "oauth",
    "typescript",
    "express",
    "security"
  ],
  "author": "AuthKit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rupe88/auth-bro.git",
    "directory": "packages/authkit"
  },
  "bugs": {
    "url": "https://github.com/Rupe88/auth-bro/issues"
  },
  "homepage": "https://github.com/Rupe88/auth-bro",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8",
    "express-rate-limit": "^7.4.1",
    "express": "^4.19.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "nodemailer": "^6.9.13",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/nodemailer": "^6.4.14"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "vitest": "^2.1.0",
    "tsup": "^8.3.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0"
  },
  "peerDependencies": {
    "@prisma/client": "^6.0.0",
    "prisma": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
